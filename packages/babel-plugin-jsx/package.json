    "@babel/template": "^7.26.9",
    "@babel/traverse": "^7.26.9",
    "@babel/types": "^7.26.9",
    "@vue/babel-helper-vue-transform-on": "workspace:*",
    "@vue/babel-plugin-resolve-type": "workspace:*",
    "@vue/shared": "^3.5.13"
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@babel/preset-env": "^7.26.9",
    "@types/babel__template": "^7.4.4",
